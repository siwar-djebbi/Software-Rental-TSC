{% extends 'base.html.twig' %}

{% block title %}Location{% endblock %}

{% block body %}
    <h1>Location</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ location.id }}</td>
            </tr>
            <tr>
                <th>Key_token</th>
                <td>{{ location.KeyToken }}</td>
            </tr>
            <tr>
                <th>Is_expired</th>
                <td>{{ location.isExpired ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Date_exp</th>
                <td>{{ location.dateExp ? location.dateExp|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_location_index') }}">back to list</a>

    <a href="{{ path('app_location_edit', {'id': location.id}) }}">edit</a>

    {{ include('location/_delete_form.html.twig') }}
{% endblock %}
